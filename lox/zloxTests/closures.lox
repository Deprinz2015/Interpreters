print("Simple Closures");
fun outer() {
    var x = "outside";
    fun inner() {
        fun theInnerest() {
            print("innerest: " + x);
        }
        theInnerest();
        print("inner: " + x);
    }
    inner();
    print("outer: " + x);
}
outer();

print("Complex Closures");
fun outer() {
  var x = "outside";
  fun inner() {
    print(x);
  }

  return inner;
}

var closure = outer();
closure();
